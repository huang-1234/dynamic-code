{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Node",
    "jsx": "react-jsx",
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "lite-store-react": ["libs/lite-store-react/src"],
      "lite-store-react/*": ["libs/lite-store-react/src/*"]
    },
    "types": ["node"],
    "esModuleInterop": true,
    "skipLibCheck": true
  },
  "include": ["libs/**/*.ts", "libs/**/*.tsx"]
}